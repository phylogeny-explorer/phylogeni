{
  "name": "phylogeni",
  "version": "0.1.0",
  "private": true,
  "author": "Phylogeny Explorer Project",
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "pnpm dlx turbo@latest run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm dlx turbo@latest lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@ark-ui/react": "^4.8.0",
    "@chakra-ui/react": "3.3.3",
    "@emailjs/browser": "^4.3.3",
    "@emotion/react": "^11.13.3",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.39.7",
    "@terminusdb/terminusdb-client": "^10.0.33",
    "cookies-next": "^4.2.1",
    "formik": "^2.4.5",
    "motion": "^11.17.0",
    "next": "^15.1.4",
    "next-themes": "^0.4.3",
    "react": "^19.0.0",
    "react-d3-tree": "^3.6.2",
    "react-dom": "^19.0.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "react-swipeable": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "use-debounce": "^10.0.3",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@commitlint/cz-commitlint": "19.5.0",
    "@playwright/test": "^1.48.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.4",
    "standard-version": "^9.5.0",
    "supabase": "^1.172.2",
    "typescript": "^5.6.3"
  }
}
